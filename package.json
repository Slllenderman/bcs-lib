        "@storybook/react": "^7.6.9",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "babel-loader": "^9.1.3",
        "husky": "^8.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-is": "^18.2.0",
        "size-limit": "^11.0.2",
        "tsdx": "^0.14.1",
        "tslib": "^2.6.2",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@bauman-conference-library/interface": "^0.0.10"
